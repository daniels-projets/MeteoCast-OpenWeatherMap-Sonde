void traductionJour() {

//   Mon=Lundi, Tue=Mardi, Wed=Mercredi, Thu=Jeudi, Fri=Vendredi, Sat=Samedi, Sun=Dimanche
     if (tempJour == "Mon") { jourTraduit = "Lundi"; }
     if (tempJour == "Tue") { jourTraduit = "Mardi"; }
     if (tempJour == "Wed") { jourTraduit = "Mercredi"; }
     if (tempJour == "Thu") { jourTraduit = "Jeudi"; }
     if (tempJour == "Fri") { jourTraduit = "Vendredi"; }
     if (tempJour == "Sat") { jourTraduit = "Samedi"; }
     if (tempJour == "Sun") { jourTraduit = "Dimanche"; }
      
}
